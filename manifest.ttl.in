  rdfs:seeAlso <osc.ttl> .

<http://colin.kinlo.ch/lv2/simple/sine>
  a lv2:Plugin ;
  lv2:binary <@OBJECT@> ;
  rdfs:seeAlso <sine.ttl> .

<http://colin.kinlo.ch/lv2/simple/mult>
  a lv2:Plugin ;

  doap:homepage <https://colin.knilo.ch/lv2/simple/> ;
  doap:maintainer [
    foaf:name "Colin Kinloch" ;
    foaf:mbox <mailto:colin@kinlo.ch>
  ] .

<http://colin.kinlo.ch/lv2/simple/osc>
